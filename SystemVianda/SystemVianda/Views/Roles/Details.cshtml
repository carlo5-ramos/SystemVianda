                        <div class="panel-heading"><h4>Detalles De Rol</h4></div>
                        <div class="panel-body">
                            <div>
                                <div>

                                    <dl class="dl-horizontal">
                                        <dt>
                                            @Html.DisplayNameFor(model => model.Rol)
